def tran(ar):
    print(ar)
    al=[[0 for _ in range(3)] for _ in range(3)]
    print(al)
    for i in range(0,3):
        roww=ar[i]
        for j in range(0,3):
            al[j][i]=roww[j]
            
    
            
            
            
    print(al)
    for i in range(0,3):
        al[i] = sorted(al[i], reverse=True)
        
        
        
    print(al)
    
    
    
            
        










arr = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
tran(arr)
